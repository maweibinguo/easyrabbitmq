{
    "name": "maweibinguo/easyrabbitmq",
    "type": "library",
    "description": "对php-amqplib/php-amqplib包的二次封装，为常见功能提供一套开箱即用的生产解决方案",
    "keywords": ["rabbitmq", "message", "queue"],
    "authors": [
        {
            "name": "maweibinguo",
            "role": "Original Maintainer"
        }
    ],
    "require": {
        "php-amqplib/php-amqplib": "v2.9.0"
    },
    "require-dev": {
        "ext-curl": "*",
        "phpunit/phpunit": "^6.5|^7.0|^9.5",
        "squizlabs/php_codesniffer": "^3.5",
        "nategood/httpful": "^0.2.20"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Maweibinguo\\EasyRabbitMq\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
